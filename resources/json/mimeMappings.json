{
	"mimeMappings": [
		{
			"pattern": "drive",
			"icon": "/images/google-drive.svg"
		},
		{
			"pattern": "application/vnd.google-apps.folder",
			"icon": "/images/folder_32.png",
			"view": "list.pug"
		},
		{
			"pattern": "text/html",
			"icon": "/images/html_32.png",
			"view": "htmlViewer.pug"
		},
		{
			"pattern": "text",
			"icon": "/images/file_32.png",
			"view": "textViewer.pug"
		},
		{
			"pattern": "application/json",
			"icon": "/images/file_32.png",
			"view": "textViewer.pug"
		},
		{
			"pattern": "image",
			"icon": "/images/image_32.png",
			"view": "imageView.pug"
		},
		{
			"pattern": "default",
			"icon": "/images/file_32.png",
			"view": "textViewer.pug"
		}
	]

